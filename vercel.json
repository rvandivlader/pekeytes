{
  "version": 2,
  "builds": [
    {
      "src": "public/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/index.html",
      "dest": "/public/index.html"
    },
    {
      "src": "/register.html",
      "dest": "/public/register.html"
    }
  ]
}
